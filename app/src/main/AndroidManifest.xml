<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetmobile4a">

    <application
        android:name=".injection.EsieaApplication"
        android:allowBackup="true"
        android:icon="@mipmap/mon_icone"
        android:label="CountriesList"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.main.MainActivityApi"></activity>
        <activity android:name=".presentation.main.MainActivityRegister"
            android:label="Main_Activity_Register"
            android:parentActivityName=".presentation.main.MainActivity"
            >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".presentation.main.MainActivity" /></activity>
        <activity android:name=".presentation.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>